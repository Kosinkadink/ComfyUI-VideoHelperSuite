{
    "main_pass":
    [
        "-n", "-c:v", "prores_ks",
        "-pix_fmt", "yuv422p10le",
        "-profile:v",
        [
            "profile",
            [
                "lt",
                "standard",
                "hq",
                {
                    "value": "4444",
                    "effects":
                    [
                        {"param": "-pix_fmt", "value": "yuva444p10le", "cond": "has_alpha"},
                        {"param": "-pix_fmt", "value": "yuv444p10le", "cond": "no_alpha"}
                    ]
                },
                {
                    "value": "4444xq",
                    "effects":
                    [
                        {"param": "-pix_fmt", "value": "yuva444p10le", "cond": "has_alpha"},
                        {"param": "-pix_fmt", "value": "yuv444p10le", "cond": "no_alpha"}
                    ]
                }
            ],
            {"default": "hq"}
        ],
        "-vf", "scale=out_color_matrix=bt709,setparams=colorspace=bt709",
        "-colorspace", "bt709", "-color_primaries", "bt709", "-color_trc", "bt709"
    ],
    "fake_trc": "bt709",
    "audio_pass": ["-c:a", "pcm_s16le"],
    "extension": "mov"
}
